
public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] a1 = new int[3];
		for (int i = 0; i < a1.length; i++) {
			System.out.println(a1[i]);
		}//for loop a1 ends
		int[] a2 = new int[3];
		for (int i = 0; i < a2.length; i++) {
			System.out.println(a2[i]);
		}//for loop a2 ends
	}//public static void main() ends

}//
