
public class doggystyle {
public static void main( String[] args){
	int monthNumber = 8;
	
	if (monthNumber >= 1 && monthNumber <=3) {
		System.out.println("You're in Quarter 1");
		
	} //Quarter 1
	else if (monthNumber >= 4 && monthNumber <=6) {
		System.out.println("You are in quarter 2");

	} // Quarter 2
	else {
		System.out.println("You are not in the first half of the year!");
		
	} //Error! You are not in the first half of the year
	
}
}
